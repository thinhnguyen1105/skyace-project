{
  "name": "demo-nextjs-v6",
  "version": "0.1.0",
  "description": "Demo project to explore NextJS v6 with custom server side rendering (SSR), Jest/Enzyme testing, code coverage reporting, and the webpack bundle analyzer.",
  "pre-commit": [
    "tslint"
  ],
  "scripts": {
    "dev": "npm run build && cross-env NODE_ENV=development node --inspect --trace-warnings .next/production-server/server/server.js -p $PORT",
    "prebuild": "shx rm -rf .next/",
    "build": "next build && tsc --project tsconfig.server.json",
    "postbuild": "shx cp -r static .next/",
    "start": "cross-env NODE_ENV=production node .next/production-server/server/server.js -p $PORT",
    "migrate-conversation": "ts-node --project tsconfig.server.json create-conversation-for-existed-bookings.ts",
    "migrate-external-user": "ts-node --project tsconfig.server.json ./scripts/import-external-users.ts",
    "update-tuitions-data": "ts-node --project tsconfig.server.json update-tuition-reference-id.ts",
    "deploy": "npm run deploy:now",
    "deploy:now": "now --public",
    "docker:up": "docker-compose up -d && docker-compose logs -f",
    "docker:up:clean": "docker-compose up --build -d && docker-compose logs -f",
    "docker:down": "docker-compose down",
    "docker:nuke": "docker rmi -f $(docker images -q)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheRobBrennan/demo-nextjs-v6.git"
  },
